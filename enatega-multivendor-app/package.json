    "axios": "^1.6.8",
    "core-js": "^3.33.3",
    "deprecated-react-native-prop-types": "^5.0.0",
    "expo": "~50.0.15",
    "expo-apple-authentication": "~6.3.0",
    "expo-application": "~5.8.3",
    "expo-auth-session": "~5.4.0",
